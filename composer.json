{
    "name": "adriandev374/autoload-routes",
    "version": "0.0.1",
    "description": "Automatic loading of route files in Laravel for multiple groups of middlewares",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "AdrianDev374",
            "email": "adriandev374@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/finder": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Adriandev374\\AutoloadRoutes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adriandev374\\AutoloadRoutes\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
